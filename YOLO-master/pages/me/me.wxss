.body {
  position: relative;
  background-image: url('https://pic.imgdb.cn/item/668632bbd9c307b7e9ef030d.jpg');
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
  overflow: hidden;
}

.body .loginPart {
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  background-color: #39235b81;
  padding: 40px 0px;
}

.body .topTitle {
  z-index: 5;
  display: inline-block;
  background-color: #39235b81;
  width: 100%;
  text-align: center;
  color: #ffffff;
  padding-top: 25px;
}

.body .loginPart .avatarurl {
  width: 90px;
  height: 90px;
  border-radius: 50%;
  margin-bottom: 10px;
}

.body .loginPart .nickname {
  color: #ffffff;
}

.loginBut {
  border:3px solid #997CA2;
  box-shadow: 1px 1px 2px 2px #a385ac8e;
  height: 40px !important;
  width: 130px !important;
  text-align: center;
  color: #ffffff;
  background-color: #997ca2a1;
  font-weight: normal;
  font-family:'Times New Roman', Times, serif;
  padding: 5px;
}

.cell-body {
  position: relative;
  background-color: #997ca24d;
  height: 420px;
}

.cell-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  width: 100%;
  top: -20px;
}

.cell {
  display: flex;
  justify-content: space-between;
  width: 90%;
  background-color: #997ca2e7;
  padding: 30rpx 20rpx;
  border-radius: 20rpx;
  align-items: center;
  margin-bottom: 30rpx;
  box-shadow: 3px 3px 5px #a26bb3;
}

.cell-left {
  display: flex;
  align-items: center;
}

.cell-left .cell-icon {
  width: 50rpx;
  height: 50rpx;
  margin-left: 10px;
}

.cell-left .cell-left-text {
  color: #ffffff;
  margin-left: 25rpx;
}

.cell-right .big-content {
  width: 35rpx;
  height: 35rpx;
  padding-right: 30rpx;
}